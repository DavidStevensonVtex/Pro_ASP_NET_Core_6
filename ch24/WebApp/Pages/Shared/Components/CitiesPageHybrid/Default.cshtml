@model CityViewModel

<table class="table table-sm table-bordered text-white bg-dark">
    <thead><tr><th colspan="2">Hybrid Page Summary</th></tr></thead>
    <tbody>
        <tr>
            <td>Cities:</td>
            <td class="text-end">@Model.Cities</td>
        </tr>
        <tr>
            <td>Population</td>
            <td class="text-end">
                @Model?.Population?.ToString("N0")
            </td>
        </tr>
    </tbody>
</table>